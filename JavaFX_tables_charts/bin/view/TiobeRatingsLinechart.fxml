<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="578.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TiobeRatingsLinechartController">
   <children>
      <HBox alignment="BOTTOM_CENTER" layoutX="2.0" layoutY="444.0" prefHeight="40.0" prefWidth="590.0" spacing="450.0">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backToTable" stylesheets="@application.css" text="Back" />
            <Button fx:id="closeButton" mnemonicParsing="false" onAction="#closeScene" stylesheets="@application.css" text="Close" />
         </children>
      </HBox>
      <LineChart fx:id="lineChart" prefHeight="454.0" prefWidth="579.0" title="TIOBE Programming Comunity Index">
        <xAxis>
          <NumberAxis label="Year" prefHeight="50.0" prefWidth="555.0" side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" label="Ratings %" side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
